{
  "name": "@recursica/internal-tool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:lib\"",
    "dev:ui": "tsc -b && vite build --watch",
    "dev:lib": "tsc -b && vite build --config vite.config.lib.ts --watch",
    "build": "concurrently \"npm run build:ui\" \"npm run build:lib\"",
    "build:ui": "tsc -b && vite build",
    "build:lib": "tsc -b && vite build --config vite.config.lib.ts",
    "zip": "npm run build && node scripts/zip.mjs",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@figma/plugin-typings": "1.113.0",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.6.1"
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "@types/archiver": "6.0.3",
    "@types/react": "19.1.7",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react-swc": "3.10.2",
    "archiver": "7.0.1",
    "concurrently": "9.0.1",
    "eslint": "9.11.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.39.1",
    "vite": "6.3.5",
    "vite-plugin-singlefile": "2.2.0"
  }
}
