{
  "name": "@recursica/official-release",
  "version": "1.0.0",
  "description": "Official Recursica design system release package containing the complete bundle, JavaScript API, and TypeScript definitions",
  "main": "recursica.js",
  "types": "recursica.d.ts",
  "exports": {
    ".": {
      "types": "./recursica.d.ts",
      "import": "./recursica.js",
      "require": "./recursica.js"
    },
    "./recursica-bundle": {
      "types": "./recursica.d.ts",
      "import": "./recursica-bundle.js",
      "require": "./recursica-bundle.js"
    },
    "./recursica": {
      "types": "./recursica.d.ts",
      "import": "./recursica.js",
      "require": "./recursica.js"
    },
    "./recursica.css": "./recursica.css",
    "./recursica-tokens.css": "./recursica-tokens.css",
    "./recursicabrand-light-theme.css": "./recursicabrand-light-theme.css",
    "./recursicabrand-dark-theme.css": "./recursicabrand-dark-theme.css"
  },
  "files": [
    "recursica.js",
    "recursica.d.ts",
    "recursica-bundle.js",
    "recursica-bundle.json",
    "recursica.css",
    "recursica-tokens.css",
    "recursicabrand-light-theme.css",
    "recursicabrand-dark-theme.css"
  ],
  "keywords": [
    "design-system",
    "tokens",
    "css-variables",
    "typescript",
    "ui-kit",
    "recursica"
  ],
  "author": "hi@borderux.com",
  "license": "MIT",
  "homepage": "https://github.com/borderux/recursica#readme",
  "bugs": {
    "url": "https://github.com/borderux/recursica/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borderux/recursica.git"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
