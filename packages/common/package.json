{
  "name": "@recursica/common",
  "description": "Shared TypeScript interfaces and types for Recursica projects",
  "version": "1.1.1",
  "private": false,
  "homepage": "https://github.com/borderux/recursica#readme",
  "bugs": {
    "url": "https://github.com/borderux/recursica/issues"
  },
  "author": "hi@borderux.com",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borderux/recursica.git"
  },
  "keywords": [
    "TypeScript",
    "Interfaces",
    "Types",
    "Shared",
    "Recursica"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": "./dist/types/index.d.ts",
    "./types/*": "./dist/types/*.d.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@recursica/schemas": "*",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "1.113.0",
    "@humanfs/node": "0.16.6",
    "@repo/eslint-config": "*",
    "@types/crypto-js": "4.2.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "9.28.0",
    "tsx": "^4.19.2",
    "typescript": "5.8.2",
    "vitest": "^3.2.3"
  },
  "license": "MIT"
}
